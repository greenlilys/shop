/* pages/myConsumer/index.wxss */
page {
    background: #eee;
}

.myteam-head {
    width: 90%;
    padding: 0 5%;
    height: 80rpx;
    line-height: 80rpx;
    background: #f8f8f8;
    font-size: 12pt;
    color: #333;
}

.myteam-body {
    width: 100%;
}

.myteam-body .rank {
    width: 96%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 13pt;
    border-radius: 12rpx 12rpx;
    text-align: center;
    margin: 15rpx auto;
    border: 2rpx #ff6801 solid;
}

.orange {
    background: #ff6801;
    color: #fff;
}

.gray {
    background: #fff;
    color: #999;
}

.myteam-body .rank-two view {
    width: 50%;
}

.rank-two view, .rank-three view {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    float: left;
}

.myteam-body .rank-three view {
    width: 33.3333333333333%;
}

.rank-three view:nth-of-type(1) {
    border-radius: 12rpx 0 0 12rpx;
}

.rank-three view:nth-of-type(3) {
    border-radius: 0 12rpx 12rpx 0;
}

.rank-two view:nth-of-type(1) {
    border-radius: 12rpx 0 0 12rpx;
}

.rank-two view:nth-of-type(2) {
    border-radius: 0 12rpx 12rpx 0;
}

.member {
    background: #fff;
    width: 90%;
    padding: 0 5%;
	 border-bottom: 1rpx solid #ddd;
}
.members {
    background: #fff;
    width: 90%;	
    padding: 0 5%;	 
	 height: 80rpx;
}

.member-head {
    width: 100%;    
    line-height: 80rpx;
    border-bottom: 1rpx solid #ddd;
	background: #fff;
}

.member-head-title {
    float: left;
    font-size: 13pt;
    color: #333;
}

.member-head-member {
    float: right;
    font-size: 12pt;
    color: #999;
}

.member-body {
    width: 100%;
    height: 160rpx;
}

.member-body-left {
    float: left;
    width: 150rpx;
    height: 150rpx;
}

.member-body-left image {
    width: 150rpx;
    height: 150rpx;
}

.member-body-center {
    float: left;
    margin: 0 10rpx 0 15rpx;
}

.member-body-centeritem {
    font-size: 13pt;
    color: #333;
    display: block;
    margin: 20rpx 0;
}

.member-body-centertime {
    font-size: 12pt;
    color: #999;
    display: block;
}

.member-body-right {
    float: right;
    text-align: right;
    border-left: 2rpx solid #ddd;
    padding-left: 30rpx;
    margin-top: 20rpx;
}

.member-body-right text {
    font-size: 12pt;
    color: #999;
    text-align: right;
    display: block;
    margin: 15rpx 0;
}
.loading {
    margin: 0 5px;
    width: 20px;
    height: 20px;
    display: inline-block;
    vertical-align: middle;
    -webkit-animation: weuiLoading 1s steps(12, end) infinite;
    animation: weuiLoading 1s steps(12, end) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
}

.loadmore {
    width: 65%;
    margin: 1.5em auto;
    line-height: 1.6em;
    font-size: 14px;
    text-align: center;
}

.loadmore_tips {
    display: inline-block;
    vertical-align: middle;
}
.dixian{
	font-size:9pt;
	/* margin-top:30rpx; */
	color:#999;
	 padding-top:30rpx; 
	  padding-bottom:30rpx; 
	text-align:center;

}
