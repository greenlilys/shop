.my_top{
    width: 100%;
    height: 200rpx;
}
page {
    height:100%;
    background:#fff;
}
.my_top_left{
    float: left;
    height: 150rpx;
    margin: 25rpx 30rpx 0 40rpx;
}
.my_top_left image{
    width: 150rpx;
    height:150rpx;
	border-radius: 50%;
}
.my_top_center{
    height: 150rpx;
    float: left;
    color: #FFF;
    margin-top:50rpx;
}
.my_top_center .my_top_title{
    display: block;
    font-size: 14pt;
}
.my_top_center .my_top_dengji{
    display: block;
    font-size: 11pt;
    margin-top: 10rpx;
}
.my_top_right{
    height: 100rpx;
    float: right;
    margin:70rpx 40rpx 0 0;
}
.my_top_right image{
    width: 60rpx;
    height: 60rpx;
}
.yue{
    width: 100%;
    height:100rpx;
    border-bottom: 2rpx solid #ddd;
}
.yue .yue_left,.jifen .jifen_left{
    height:60rpx;
    float: left;
    font-size: 13pt;
    color: #666;
    margin:30rpx 0 0 40rpx; 
}
.yue .yue_color{
    font-size: 12pt;
    color: #f9ab0b;
}
.yue .yue_right{
    width:120rpx;
    height:70rpx;
    line-height:70rpx; 
    float: right;
    border-radius:10rpx 10rpx;
    background: #ff6600;
    color: #FFF;
    font-size: 13pt;
    text-align: center;
    margin: 15rpx 40rpx 0 0; 
}
.jifen{
    width: 100%;
    height:100rpx;
	border-bottom: 1rpx solid #ededed;
}
.jifen .jifen_num{
    font-size: 12pt;
}
.line {
    width: 100%;
    background: #efefef;
    height: 20rpx;
}
.my_user {
    width:90%;
    background-color: #fff;    
    overflow: hidden; 
    padding: 0 5%;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1rpx solid #ededed;
}
.my_user .u-list-main{
    width: 40%;
    float: left;
}
.u-list-main image {
    width: 58rpx;
    height: 60rpx;
    float: left;
    margin: 22rpx 15rpx 0 0px;
}
.u-list-main text {
    color: #5a5959;
    float:left;
    font-size: 12pt;
    display: block;
}
.my_user .my_user_allorder{
    width: 50%;
    float: right;
}
.my_user .my_user_allorder text{
    float: left;
    font-size: 12pt;
    display: block;
    color: #999;
}
.my_user .my_user_allorder image{
    float: right;
    width:40rpx;
    height:40rpx;
    margin-top:30rpx;
}
.user-nav {
    width: 100%;
    height: 180rpx;
    background-color: #fff;   
    overflow: hidden;
    margin: 0 auto;
    padding-top: 30rpx;
}
.user-nav navigator {
    width: 25%;
    float: left;
    color: #5a5959;
    font-size: 11pt;
    text-align: center;
}
.user-nav image {
    width: 100rpx;
    height: 100rpx;
    margin: 0 auto;
    display: block;
}
.td-bt {
    width: 90%;
    height: 100rpx;
    line-height: 100rpx;
    margin: 0 auto;
    border-bottom: 1rpx solid #ededed;
    background:#fff;
}
.td-bt navigator {
    width: 100%;
    height: 100%;
}
.td-icon {
    float: left;
    width:60rpx;
    height:60rpx;
    margin: 20rpx 12rpx 0 0;
}
.td-bt text {
    float: left;
    font-size: 12pt;
    color: #666;
}
.next {
    float: right;
    width: 50rpx;
    height: 50rpx;
    margin-top: 25rpx;
}
.navigator-hover {
   opacity:0.7;
   background-color: #fff;
}